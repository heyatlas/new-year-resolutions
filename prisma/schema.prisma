// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Resolution {
  id          Int      @id @default(autoincrement())
  createdAt   DateTime @default(now())
  name        String
  character   Int
  objectives  String[]
  atlasWish   String
  likes       Int      @default(0)
} 